<%- model_class = Cost -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:estimated_cost) %>:</strong></dt>
  <dd><%= @cost.estimated_cost %></dd>
  <dt><strong><%= model_class.human_attribute_name(:from_district_id) %>:</strong></dt>
  <dd><%= @cost.from_district_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:to_district_id) %>:</strong></dt>
  <dd><%= @cost.to_district_id %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              costs_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_cost_path(@cost), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              cost_path(@cost),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
